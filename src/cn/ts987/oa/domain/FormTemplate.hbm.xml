<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.ts987.oa.domain">

	<class name="FormTemplate" table="ts_formTemplate">
		<id name="id">
			<generator class="native"/>
		</id>
		<property name="name" type="java.lang.String" length="64"/>
		<property name="pdKey" type="java.lang.String" length="64"/>
		<property name="path" type="java.lang.String" length="256"/>
		
		
	</class>
	

</hibernate-mapping>
